{
    "name": "sotnaspina/laravel-menu-security",
    "description": "Manipulation of menu, user and sector permissions for Laravel 5.3 and higher",
    "keywords": [
        "sotnas",
        "laravel",
        "sector",
        "menu",
        "rules",
        "permission",
        "acl",
        "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sotnas Leunam Pina",
            "email": "sotnas.pina@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SotnasPina\\MenuSecurity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SotnasPina\\MenuSecurity\\Test\\": "tests"
        }
    },
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit" : "^5.7|6.2|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SotnasPina\\MenuSecurity\\MenuSecurityServiceProvider"
            ]
        }
    }
}
